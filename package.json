{
  "name": "rxjs-etl-kit",
  "version": "2.0.27",
  "description": "ETL extension for RxJs library",
  "type": "module",
  "--main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./Trello": "./dist/endpoints/trello/index.js",
    "./Csv": "./dist/endpoints/csv.js",
    "./Filesystem": "./dist/endpoints/filesystem.js",
    "./Interval": "./dist/endpoints/interval.js",
    "./Json": "./dist/endpoints/json.js",
    "./Magento": "./dist/endpoints/magento.js",
    "./Memory": "./dist/endpoints/memory.js",
    "./Mysql": "./dist/endpoints/mysql.js",
    "./Postgres": "./dist/endpoints/postgres.js",
    "./Telegram": "./dist/endpoints/telegram.js"
  },
  "types": "./dist/index.d.ts",
  "node": ">=16",
  "scripts": {
    "start": "ts-node --esm src/main.ts",
    "build": "tsc",
    "test": "NODE_ENV=test jest",
    "pub:npm": "yarn publish",
    "pub:coverage": "./node_modules/.bin/codecov --token=$RXJS_ETL_KIT_CODECOV_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igor-berezhnoy/rxjs-etl-kit.git"
  },
  "bugs": {
    "url": "https://github.com/igor-berezhnoy/rxjs-etl-kit/issues"
  },
  "homepage": "https://github.com/igor-berezhnoy/rxjs-etl-kit#readme",
  "keywords": [
    "rxjs",
    "etl",
    "stream",
    "typescript",
    "javascript",
    "node",
    "node.js",
    "google translate",
    "magento",
    "trello",
    "telegram",
    "csv",
    "json",
    "jsonpath",
    "xml",
    "xpath",
    "database",
    "postgresql",
    "mysql",
    "queue",
    "extract",
    "validate",
    "transform",
    "load",
    "lib"
  ],
  "author": "Berezhnoy I.A.",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/translate": "^7.2.0",
    "chalk": "^5.0.1",
    "console-gui-tools": "^3.0.2",
    "csv": "^6.2.10",
    "dotenv": "^16.0.3",
    "glob": "^8.0.3",
    "jsonpath-plus": "^7.2.0",
    "lodash": "^4.17.21",
    "mysql2-async": "^1.1.10",
    "node-fetch": "2.6.9",
    "node-telegram-bot-api": "^0.59.0",
    "open": "^9.1.0",
    "parse-database-url": "^0.3.0",
    "pg": "^8.8.0",
    "rxjs": "^7.8.0",
    "signal-promise": "^1.0.3",
    "ts-jest-resolver": "^2.0.1",
    "xmldom-ts": "^0.3.1",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.17",
    "@types/node-telegram-bot-api": "^0.57.1",
    "codecov": "^3.8.3",
    "jest": "^29.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}