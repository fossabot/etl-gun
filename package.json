{
  "name": "rxjs-etl-kit",
  "version": "2.0.43",
  "description": "ETL extension for RxJs library",
  "type": "module",
  "--main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./Errors": "./dist/endpoints/errors.js",
    "./Trello": "./dist/endpoints/trello/index.js",
    "./Zendesk": "./dist/endpoints/zendesk/index.js",
    "./Csv": "./dist/endpoints/csv.js",
    "./Xml": "./dist/endpoints/xml.js",
    "./Json": "./dist/endpoints/json.js",
    "./Filesystem": "./dist/endpoints/filesystem.js",
    "./Interval": "./dist/endpoints/interval.js",
    "./Magento": "./dist/endpoints/magento.js",
    "./Memory": "./dist/endpoints/memory.js",
    "./databases": "./dist/endpoints/databases/index.js",
    "./Telegram": "./dist/endpoints/telegram.js"
  },
  "types": "./dist/index.d.ts",
  "node": ">=16",
  "scripts": {
    "start": "ts-node --esm src/main.ts",
    "build": "tsc",
    "test": "NODE_ENV=test jest",
    "pub:npm": "yarn publish",
    "pub:coverage": "./node_modules/.bin/codecov --token=$RXJS_ETL_KIT_CODECOV_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igor-berezhnoy/rxjs-etl-kit.git"
  },
  "bugs": {
    "url": "https://github.com/igor-berezhnoy/rxjs-etl-kit/issues"
  },
  "homepage": "https://github.com/igor-berezhnoy/rxjs-etl-kit#readme",
  "keywords": [
    "rxjs",
    "etl",
    "stream",
    "typescript",
    "node",
    "nodejs",
    "node.js",
    "google translate",
    "magento",
    "trello",
    "zendesk",
    "telegram",
    "csv",
    "json",
    "jsonpath",
    "xml",
    "xpath",
    "database",
    "CockroachDb",
    "mariadb",
    "mssql",
    "mysql",
    "oracle",
    "postgres",
    "redshift",
    "sqllite",
    "queue",
    "extract",
    "validate",
    "transform",
    "load",
    "lib"
  ],
  "author": "Berezhnoy I.A.",
  "license": "MIT",
