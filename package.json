    "console-gui-tools": "^3.0.2",
    "csv": "^6.3.0",
    "dotenv": "^16.0.3",
    "glob": "^8.0.3",
    "jsonpath-plus": "^7.2.0",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "node-fetch": "2.6.9",
    "node-telegram-bot-api": "^0.61.0",
    "open": "^9.1.0",
    "parse-database-url": "^0.3.0",
    "rxjs": "^7.8.1",
    "signal-promise": "^1.0.3",
    "xmldom-ts": "^0.3.1",
    "xpath": "^0.0.32"
  },
