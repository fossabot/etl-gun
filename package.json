{
  "name": "etl-gun",
  "version": "2.1.6",
  "description": "ETL toolkit which supports RxJS streams, error handling, business rules and many more",
  "type": "module",
  "--main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./Errors": "./dist/endpoints/errors.js",
    "./Trello": "./dist/endpoints/trello/index.js",
    "./Zendesk": "./dist/endpoints/zendesk/index.js",
    "./Csv": "./dist/endpoints/csv.js",
    "./Xml": "./dist/endpoints/xml.js",
    "./Json": "./dist/endpoints/json.js",
    "./Filesystem": "./dist/endpoints/filesystem.js",
    "./Interval": "./dist/endpoints/interval.js",
    "./Magento": "./dist/endpoints/magento.js",
    "./Memory": "./dist/endpoints/memory.js",
    "./databases": "./dist/endpoints/databases/index.js",
    "./Telegram": "./dist/endpoints/telegram.js"
  },
  "types": "./dist/index.d.ts",
  "node": ">=16",
  "scripts": {
    "start": "ts-node --esm src/main.ts",
    "build": "tsc",
    "-test": "NODE_ENV=test jest",
    "-test:clear": "jest --clearCache",
    "-test:node2": "node --test --require ts-node/register ./src/tests/**/*.test.ts",
    "test": "node --test --loader ts-node/esm ./src/tests/**/*.test.ts",
    "spec": "node --test --loader ts-node/esm --test-reporter spec ./src/tests/**/*.test.ts",
    "tap": "node --test --loader ts-node/esm --test-reporter tap ./src/tests/**/*.test.ts | npx faucet",
    "pub:npm": "yarn publish",
    "pub:coverage": "eval $(grep '^CODECOV_TOKEN' .env) && ./node_modules/.bin/codecov --token=$CODECOV_TOKEN"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/top-guns/etl-gun.git"
  },
  "bugs": {
    "url": "https://github.com/top-guns/etl-gun/issues"
  },
  "homepage": "https://github.com/top-guns/etl-gun#readme",
  "keywords": [
    "rxjs",
    "etl",
    "stream",
    "typescript",
    "node.js",
    "google translate",
    "magento",
    "trello",
    "zendesk",
    "telegram",
    "csv",
    "json",
    "jsonpath",
    "xml",
    "xpath",
    "database",
    "CockroachDb",
    "mariadb",
    "mssql",
    "mysql",
    "oracle",
    "postgres",
    "redshift",
    "sqllite",
    "queue",
    "extract",
    "load",
    "business rules"
  ],
  "author": "Berezhnoy I.A.",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/translate": "^7.2.1",
    "basic-ftp": "^5.0.3",
    "chalk": "^5.2.0",
    "console-gui-tools": "^3.0.2",
    "csv": "^6.3.0",
    "dotenv": "^16.0.3",
    "glob": "^10.2.6",
    "jsonpath-plus": "^7.2.0",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "node-fetch": "2.6.9",
    "node-telegram-bot-api": "^0.61.0",
    "open": "^9.1.0",
    "parse-database-url": "^0.3.0",
    "rools": "^2.3.0",
    "rxjs": "^7.8.1",
    "signal-promise": "^1.0.3",
    "ssh2-sftp-client": "^9.1.0",
    "stream-buffers": "^3.0.2",
    "webdav": "^5.0.0",
    "xmldom-ts": "^0.3.1",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@types/node-telegram-bot-api": "^0.61.6",
    "@types/ssh2-sftp-client": "^9.0.0",
    "@types/stream-buffers": "^3.0.4",
    "codecov": "^3.8.3",
    "faucet": "^0.0.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.3.1",
    "pg": "^8.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "peerDependenciesMeta": {
    "tedious": {
      "optional": true
    },
    "mysql": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "pg-native": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    }
  }
}